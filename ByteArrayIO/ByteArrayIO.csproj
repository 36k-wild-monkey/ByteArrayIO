<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>ByteArray from node.js Buffer</Description>
    <Authors>wlpha</Authors>
    <Company>wlpha</Company>
    <Product>ByteArrayIO</Product>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Copyright Â© 2020</Copyright>
    <PackageId>ByteArrayIO</PackageId>
    <PackageProjectUrl>https://github.com/wlpha/ByteArrayIO.git</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/wlpha/ByteArrayIO.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>1.0.0</Version>
  </PropertyGroup>

</Project>
